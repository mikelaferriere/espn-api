{
  "name": "@mikelaferriere/espn-api",
  "version": "0.2.0",
  "description": "Typescript library for accessing ESPN's Public API",
  "keywords": [],
  "author": "Mike Laferriere",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://git@github.com/mikelaferriere/espn-api.git"
  },
  "main": "build/lib/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "tsc",
    "deploy": "rollup -c --bundleConfigAsCjs",
    "format:check": "prettier . --check",
    "format:fix": "prettier --write .",
    "lint": "eslint .",
    "predeploy": "rm -rf build && npm run build",
    "precommit": "npm run format:check && npm run lint && npm run build && npm run test",
    "publish": "changeset publish",
    "changeset": "changeset",
    "test": "jest . --coverage"
  },
  "dependencies": {
    "axios": "1.6.2",
    "luxon": "3.4.4"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@types/jest": "29.5.7",
    "@types/luxon": "3.3.4",
    "@types/node": "18.6.0",
    "@typescript-eslint/parser": "6.9.1",
    "eslint": "8.52.0",
    "jest": "29.7.0",
    "prettier": "3.0.3",
    "rollup": "^4.4.1",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-node-dev": "2.0.0",
    "typescript": "5.2.2"
  }
}
