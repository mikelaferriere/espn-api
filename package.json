{
  "name": "@mikelaferriere/espn-api",
  "version": "0.1.2",
  "description": "Typescript library for accessing ESPN's Public API",
  "keywords": [],
  "author": "Mike Laferriere",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://git@github.com/mikelaferriere/espn-api.git"
  },
  "main": "build/lib/index.js",
  "exports": {
    ".": {
      "types": {
        "default": "./index.d.ts"
      }
    },
    "./package.json": "./package.json"
  },
  "types": "./index.d.ts",
  "files": [
    "build",
    "lib"
  ],
  "scripts": {
    "build": "tsc",
    "deploy": "tsc",
    "format:check": "prettier . --check",
    "format:fix": "prettier --write .",
    "lint": "eslint .",
    "postdeploy": "rm -rf node_modules",
    "prebuild": "rm -rf build",
    "precommit": "npm run format:check && npm run lint && npm run build && npm run test",
    "publish": "changeset publish",
    "changeset": "changeset",
    "test": "jest . --coverage"
  },
  "dependencies": {
    "axios": "1.6.2",
    "luxon": "3.4.4"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@types/jest": "29.5.7",
    "@types/luxon": "3.3.4",
    "@types/node": "18.6.0",
    "@typescript-eslint/parser": "6.9.1",
    "eslint": "8.52.0",
    "jest": "29.7.0",
    "prettier": "3.0.3",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-node-dev": "2.0.0",
    "typescript": "5.2.2"
  }
}
