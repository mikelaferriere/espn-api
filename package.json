{
  "name": "@mikelaferriere/espn-api",
  "version": "0.5.0",
  "description": "Typescript library for accessing ESPN's Public API",
  "keywords": [],
  "author": "Mike Laferriere",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://git@github.com/mikelaferriere/espn-api.git"
  },
  "main": "build/lib/index.js",
  "types": "build/dts",
  "files": [
    "build",
    "lib"
  ],
  "scripts": {
    "build": "tsc",
    "build:examples": "tsc examples/*",
    "changeset": "changeset",
    "deploy": "tsc",
    "format:check": "prettier . --check",
    "format:fix": "prettier --write .",
    "lint": "eslint .",
    "predeploy": "rm -rf build && npm run build && mv build/dts/lib/* build/dts/",
    "precommit": "npm run format:check && npm run lint && npm run build && npm run test",
    "publish": "changeset publish",
    "docs": "rm -rf docs && typedoc lib/index.ts --out docs",
    "test": "jest . --coverage"
  },
  "dependencies": {
    "axios": "1.6.2",
    "luxon": "3.4.4"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@types/jest": "29.5.7",
    "@types/luxon": "3.3.4",
    "@types/node": "^20.9.1",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.9.1",
    "esbuild": "^0.19.5",
    "eslint": "8.52.0",
    "jest": "29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "3.0.3",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "2.0.0",
    "typedoc": "^0.25.3",
    "typescript": "5.2.2"
  }
}
